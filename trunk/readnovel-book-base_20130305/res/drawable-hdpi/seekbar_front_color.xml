<?xml version="1.0" encoding="UTF-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ff9d9e9d" android:endColor="#ff9d9e9d" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffffffff" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#80ffd300" android:endColor="#80ffb600" android:angle="270.0" android:centerY="0.75" android:centerColor="#a0ffcb00" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#80000000" android:endColor="#a0000000" android:angle="270.0" android:centerY="0.75" android:centerColor="#80000000" />
            </shape>
        </clip>
    </item>
</layer-list>