<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:orientation="vertical"
  android:background="#e9e9e9"
  >
  	<include
		layout="@layout/include_top_bar"
		android:id="@+id/paihang_top_bar" />
		
	 <LinearLayout
	 android:layout_gravity="center_horizontal"
	  android:layout_width="wrap_content"
	  android:layout_height="76.6dip"
	  android:orientation="horizontal">
		  <ImageView 
		  android:layout_width="142dip"
		  android:layout_height="62dip"
		  android:layout_marginRight="6dip"
		  android:layout_gravity="center_vertical"
		  android:id="@+id/menu_paihang_totle"/>
		  <ImageView 
		  android:layout_width="142dip"
		  android:layout_height="62dip"
		  android:layout_marginLeft="6dip"
		  android:layout_gravity="center_vertical"
		  android:id="@+id/menu_paihang_vip"/>
	  </LinearLayout>
	  
	  <View 
	  android:layout_width="fill_parent"
	  android:layout_height="1dip"
	  android:id="@+id/paihang_line1"
	  android:visibility="visible"
	  android:background="@drawable/line_q"/>
	  
	  <LinearLayout	
	   android:layout_gravity="center_horizontal"
	  android:layout_width="wrap_content"
	  android:layout_height="76.6dip"
	  android:orientation="horizontal">
		  <ImageView 
		  android:layout_width="142dip"
		  android:layout_height="62dip"
		  android:layout_marginRight="6dip"
		  android:layout_gravity="center_vertical"
		  android:id="@+id/menu_paihang_over"/>
		  <ImageView 
		  android:layout_width="142dip"
		  android:layout_height="62dip"
		  android:layout_marginLeft="6dip"
		  android:layout_gravity="center_vertical"
		  android:id="@+id/menu_paihang_mustread"/>
	  </LinearLayout>
	  
	  <View 
	  android:layout_width="fill_parent"
	  android:layout_height="1dip"
	  android:id="@+id/paihang_line2"
	  android:visibility="visible"
	  android:background="@drawable/line_q"/>

	  <ListView
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:cacheColorHint="#00000000"
	android:divider="@drawable/line_q"
	android:layout_marginBottom="50dp"
	android:scrollbars="none"
	android:id="@+id/menu_paihang_listview">
	  </ListView>
</LinearLayout>